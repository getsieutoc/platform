    "@emotion/styled": "11.11.0",
    "@next-auth/prisma-adapter": "1.0.7",
    "@octokit/auth-app": "6.0.1",
    "@octokit/core": "5.0.1",
    "@prisma/client": "5.5.2",
    "@upstash/ratelimit": "0.4.4",
