{
  "private": true,
  "scripts": {
    "prisma": "prisma",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push --skip-generate",
    "prisma:dev": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "postinstall": "pnpm prisma:generate",
    "clean": "rimraf node_modules .next dist && pnpm install",
    "dev": "docker-compose up -d && pnpm run watch && docker-compose down",
    "watch": "trap 'exit 0' SIGINT; pnpm run prisma:generate && pnpm run prisma:push && next dev",
    "build": "pnpm run prisma:generate && next build",
    "format:write": "prettier --write \"**/*.{css,js,json,jsx,ts,tsx}\"",
    "format": "prettier \"**/*.{css,js,json,jsx,ts,tsx}\"",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
