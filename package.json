    "framer-motion": "10.16.4",
    "js-cookie": "3.0.5",
    "nanoid": "5.0.3",
    "next": "14.0.2",
    "next-auth": "4.24.5",
    "octokit": "3.1.1",
    "openai-edge": "1.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.11.0",
